{
    "name":         "seo2websites/mink-browserkit-driver",
    "require": {
        "php": ">=8.2",
        "ext-dom": "*",
        "behat/mink": "^1.11.0@dev",
        "seo2websites/browser-kit": "dev-master",
        "wa72/htmlpagedom": "dev-master",
        "symfony/dom-crawler": "^7.1"
    },
    "require-dev": {
        "mink/driver-testsuite": "dev-master",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^8.5 || ^9.5",
        "symfony/error-handler": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/http-client": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/http-kernel": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/mime": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Behat\\Mink\\Driver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Behat\\Mink\\Tests\\Driver\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:MMERP/seo2websites-browser-kit"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": false
}
